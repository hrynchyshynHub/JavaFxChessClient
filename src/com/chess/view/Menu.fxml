<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800.0" prefWidth="1400.0" xmlns="http://javafx.com/javafx/8.0.66" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.chess.MenuControlller">
   <children>
      <Label contentDisplay="CENTER" layoutX="210.0" layoutY="14.0" prefHeight="38.0" prefWidth="189.0" text="Main Menu" textAlignment="CENTER">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <Button fx:id="btnNewGame" layoutX="219.0" layoutY="72.0" mnemonicParsing="false" onAction="#newGame" prefWidth="110.0" text="New Game" />
      <Button fx:id="btnLoadGame" layoutX="219.0" layoutY="112.0" mnemonicParsing="false" onAction="#loadGame" prefWidth="110.0" text="Load Game" />
      <Button fx:id="btnExit" layoutX="219.0" layoutY="152.0" mnemonicParsing="false" onAction="#exit" prefWidth="110.0" text="Exit" />
   </children>
</AnchorPane>
